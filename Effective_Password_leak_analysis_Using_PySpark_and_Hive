t set for leaked passwords\n","top_pw_list = [r[\"password\"] for r in top_df.limit(300000).collect()]\n","top_pw_set = set(top_pw_list)\n","broadcast_top = spark.sparkContext.broadcast(top_pw_set)\n","\n","def evaluate_password(pw):\n","    reasons = []\n","    ent = entropy_py(pw)\n","    pw_low = pw.lower()\n","\n","    # Length checks\n","    if len(pw) < 8:\n","        reasons.append(\"Too short (less than 8 characters)\")\n","    elif len(pw) < 12:\n","        reasons.append(\"Length is moderate (8–11 chars) — recommended 12+\")\n","\n","    # Entropy checks\n","    if ent < 3:\n","        reasons.append(f\"Very low entropy ({ent:.2f} bits)\")\n","    elif ent < 4:\n","        reasons.append(f\"Low entropy ({ent:.2f} bits)\")\n","\n","    # Composition checks\n","    if pw.islower():\n","        reasons.append(\"Contains only lowercase letters\")\n","    if pw.isupper():\n","        reasons.append(\"Contains only uppercase letters\")\n","    if pw.isdigit():\n","        reasons.append(\"Contains only digits\")\n","    if not pw.isalnum():\n","        # contains special characters, no need to add a reason\n","        pass\n","\n","    # Repeated characters\n","    if len(set(pw)) == 1:\n","        reasons.append(\"Contains repeated characters (e.g., aaaaaaa)\")\n","\n","    # Sequential pattern check\n","    seq_chars = \"abcdefghijklmnopqrstuvwxyz0123456789\"\n","    for i in range(len(pw_low) - 2):\n","        if pw_low[i:i+3] in seq_chars:\n","            reasons.append(\"Contains sequential pattern (e.g., abc or 123)\")\n","            break\n","\n","    # Keyboard pattern\n","    keyboard_patterns = [\"qwerty\", \"asdf\", \"zxcv\", \"12345\", \"password\", \"letmein\", \"admin\"]\n","    if any(p in pw_low for p in keyboard_patterns):\n","        reasons.append(\"Contains keyboard pattern (qwerty, asdf, 12345, etc.)\")\n","\n","    # Year pattern\n","    if re.search(r\"(19\\d\\d|20\\d\\d)\", pw):\n","        reasons.append(\"Contains a year (e.g., 1999 or 2023)\")\n","\n","    # Leaked dataset check\n","    if pw in broadcast_top.value:\n","        reasons.append(\"This password appears in leaked datasets\")\n","\n","    # Final status\n","    status = \"Safe\" if len(reasons) == 0 else \"Vulnerable\"\n","    return status, reasons\n","\n","\n","\n","def check_password_input():\n","    pw = input(\"Enter a password to evaluate: \")\n","    status, reasons = evaluate_password(pw)\n","\n","    print(\"\\nPassword:\", pw)\n","    print(\"Status:\", status)\n","    print(\"----------------------------------------\")\n","\n","    if status == \"Safe\":\n","        print(\"✔ No major vulnerabilities found. Good password!\")\n","    else:\n","        print(\"⚠ Vulnerabilities found:\")\n","        for r in reasons:\n","            print(\" -\", r)\n"],"metadata":{"id":"aVCJyQtikm6V","executionInfo":{"status":"ok","timestamp":1763966708191,"user_tz":-330,"elapsed":38942,"user":{"displayName":"Saad Shikalgar","userId":"11699813085101937601"}}},"execution_count":19,"outputs":[]},{"cell_type":"code","source":["evaluate_password(\"password123\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"e72dM0mDl6IV","executionInfo":{"status":"ok","timestamp":1763966735718,"user_tz":-330,"elapsed":795,"user":{"displayName":"Saad Shikalgar","userId":"11699813085101937601"}},"outputId":"437e2ecd-67ab-4226-e017-36a610f70df3"},"execution_count":21,"outputs":[{"output_type":"execute_result","data":{"text/plain":["('Vulnerable',\n"," ['Length is moderate (8–11 chars) — recommended 12+',\n","  'Low entropy (3.28 bits)',\n","  'Contains only lowercase letters',\n","  'Contains sequential pattern (e.g., abc or 123)',\n","  'Contains keyboard pattern (qwerty, asdf, 12345, etc.)'])"]},"metadata":{},"execution_count":21}]}]}