len(s)\n","    return float(-sum((v/l)*math.log2(v/l) for v in c.values()))\n","\n","entropy_udf = udf(entropy_py, FloatType())\n","\n","keyboard_patterns = [\"qwerty\",\"asdf\",\"zxcv\",\"12345\",\"password\",\"letmein\",\"admin\"]\n","year_re = re.compile(r\"(19\\d\\d|20\\d\\d)\")\n","\n","def has_kb(s):\n","    return any(p in (s or \"\").lower() for p in keyboard_patterns)\n","\n","def has_year(s):\n","    return bool(year_re.search(s or \"\"))\n","\n","kb_udf = udf(has_kb, BooleanType())\n","year_udf = udf(has_year, BooleanType())\n","\n","pw_df = pw_df \\\n","    .withColumn(\"length\", length(col(\"password\"))) \\\n","    .withColumn(\"entropy\", entropy_udf(col(\"password\"))) \\\n","    .withColumn(\"has_keyboard\", kb_udf(col(\"password\"))) \\\n","    .withColumn(\"has_year\", year_udf(col(\"password\")))\n"],"metadata":{"id":"Q56H-QEDbwOw","executionInfo":{"status":"ok","timestamp":1763965576377,"user_tz":-330,"elapsed":604,"user":{"displayName":"Saad Shikalgar","userId":"11699813085101937601"}}},"execution_count":5,"outputs":[]},{"cell_type":"code","source":["pw_df = pw_df.withColumn(\n","    \"composition\",\n","    when(col(\"password\").rlike(\"^[0-9]+$\"), \"digits_only\")\n","    .when(col(\"password\").rlike(\"^[a-z]+$\"), \"lower_only\")\n","    .when(col(\"password\").rlike(\"^[A-Z]+$\"), \"upper_only\")\n","    .when(col(\"password\").rlike(\"^[^a-zA-Z0-9]+$\"), \"special_only\")\n","    .otherwise(\"mixed\")\n",")\n"],"metadata":{"id":"jfeHsk5MdI7H","executionInfo":{"status":"ok","timestamp":1763965592861,"user_tz":-330,"elapsed":724,"user":{"displayName":"Saad Shikalgar","userId":"11699813085101937601"}}},"execution_count":6,"outputs":[]},{"cell_type":"code","source":["from pyspark.sql.functions import count, desc\n","\n","spark.sql(\"CREATE DATABASE IF NOT EXISTS password_analysis\")\n","\n","top_df = (\n","    pw_df.groupBy(\"password\")\n","    .agg(count(\"*\").alias(\"count\"))\n","    .orderBy(desc(\"count\"))\n",")\n","\n","top_df.wri